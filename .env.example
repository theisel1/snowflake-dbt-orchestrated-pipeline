# Snowflake connection (password auth)
SNOWFLAKE_ACCOUNT=xy12345.us-east-1
SNOWFLAKE_USER=your_username
SNOWFLAKE_ROLE=ACCOUNTADMIN
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_AUTHENTICATOR=snowflake
SNOWFLAKE_PASSWORD=your_password

# For PAT / OAuth auth, use this instead of password auth:
# SNOWFLAKE_AUTHENTICATOR=oauth
# SNOWFLAKE_TOKEN=your_programmatic_access_token

# Pipeline defaults
SNOWFLAKE_DATABASE=PORTFOLIO_DB
SNOWFLAKE_SCHEMA_RAW=RAW
SNOWFLAKE_SCHEMA_STAGING=STAGING
SNOWFLAKE_SCHEMA_MARTS=MARTS
SNOWFLAKE_QUERY_TAG=snowflake-dbt-orchestrated-pipeline

# Snowflake native dbt project defaults
SNOWFLAKE_DBT_PROJECT_FQN=PORTFOLIO_DB.MARTS.TRIPS_DBT_PROJECT
SNOWFLAKE_DBT_GIT_REPO_FQN=PORTFOLIO_DB.RAW.PORTFOLIO_GIT_REPO
SNOWFLAKE_DBT_BRANCH=main
SNOWFLAKE_DBT_PROJECT_ROOT=dbt
SNOWFLAKE_DBT_DEFAULT_TARGET=prod
SNOWFLAKE_DBT_ARGS=build --target prod
# Optional, comma-separated list
# SNOWFLAKE_DBT_EXTERNAL_ACCESS_INTEGRATIONS=DBT_EXTERNAL_ACCESS_INT

# Dagster Cloud metadata used in CI/deploy docs
DAGSTER_CLOUD_ORGANIZATION=th
DAGSTER_CLOUD_DEPLOYMENT=prod
